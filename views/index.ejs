    <% for(post of posts ){ %>
        <div class="posts">
            <h3 class="user">@<%= post.username %>
            </h3>
            <h4 class="content">
                <%= post.content %>
            </h4>
            <div class="read-more">
                 <a href="http://localhost:8080/posts/<%=post.id%>">READ MORE ></a>
                 <a href="http://localhost:8080/posts/<%=post.id%>/edit">EDIT ^</a>
            </div>
           
        </div>
        <% } %>
            <br><br>
            <div id="newpost">
                <a href="http://localhost:8080/posts/new">+ New Post</a>
            </div>
            

